{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "ignoreDeprecations": "6.0",
    "paths": {
      "@/*": [
        "src/*",
        "src/components/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "react",
    "moduleResolution": "bundler",
    "types": [
      "astro/client"
    ]
  }
}